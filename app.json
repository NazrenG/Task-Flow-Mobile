{
  "expo": {
    "name": "Task-Flow-Mobile",
    "slug": "Task-Flow-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gif/loading.gif",
    "scheme": "taskflowmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "foregroundImage": "./assets/gif/loading.gif",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/gif/loading.gif",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/gif/loading.gif"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/gif/loading.gif",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo": {
      "web": {
        "bundler": "metro"
      }
    }
  }
}
